page{
  height: 88%;
  width: 96%;
  padding: 10% 2% 2% 2%;
}
.btn{
  height: 300rpx;
  width: 300rpx;
  border-radius: 150rpx;
  padding: 0;
  color: white;
  font-size: 1.5rem;
  font-weight: bold;
  letter-spacing: 5rpx;  
  line-height: 300rpx;
  background: linear-gradient(to bottom right, rgb(20, 150, 106), rgb(41, 230, 173));
  vertical-align: middle; 
  border: 2rpx rgb(60, 189, 173) solid;
  box-shadow: 5px 10px 15px 2px rgba(37, 148, 27, 0.1);
}
.btn:hover{
  background-color: #d5d5d5;
}
.log{
  text-align: right;
  font-size: 32rpx;
  color: rgb(134, 134, 134);
  font-weight: bold;
  letter-spacing: 2rpx;
  margin: 50rpx 10rpx 30rpx 0;
}
.log:hover{
  color: black;
}
map{
  width: 98%;
  margin: auto;
  height: 700rpx;
}