.border-null:after {
  border:none;
  height: 0;
  width: 0;
  content: " ";
  position: absolute;
}

.mb {
  margin-bottom:150rpx;
}





.weui-cells-border {
  position: relative;
}

.weui-cells-border:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #D9D9D9;
  color: #D9D9D9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.weui-cells-border:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #D9D9D9;
  color: #D9D9D9;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}



.weui-cells .weui-cell-null::before {
  content:" ";
  position:absolute;
  left:0;
  top:0;
  right:0;
  height:0rpx;
  border-top:0rpx solid #D9D9D9;
  color:#D9D9D9;
  -webkit-transform-origin:0 0;
  transform-origin:0 0;
  -webkit-transform:scaleY(0.5);
  transform:scaleY(0.5);
}
